{
  "invocation": "virtual krus",
  "version": "4.0",
  "intents": {
    "AskNameIntent": {
      "phrases": [
        "{name}",
        "my name is {name}",
        "i am {name}",
        "you can call me {name}"
      ],
      "entities": {
        "name": {
          "type": {
            "googleAssistant": "NameInputType",
            "alexa": "AMAZON.US_FIRST_NAME",
            "dialogflow": "@sys.given-name"
          }
        }
      }
    },
    "YesIntent": {
      "phrases": [
        "yes",
        "yes good",
        "sure"
      ]
    },
    "NoIntent": {
      "phrases": [
        "no",
        "not okay"
      ]
    }
  },
  "entityTypes": {
    "NameInputType":{
      "values": [
        {
          "value": "max"
        },
        {
          "value": "jane"
        },
        {
          "value": "joe"
        }
      ]
    }
  }
}
